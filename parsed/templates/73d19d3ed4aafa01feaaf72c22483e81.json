{"components/faceted-search/facets/rating":"<ul class=\"facet-filter-list\n  {{#if selected}}is-open{{/if}}\n  {{#unless start_collapsed}}is-open{{/unless}}\"\n  data-facet-filter-wrapper>\n\n  {{#each items}}\n    <li class=\"rating-facet {{#if selected}}facet-selected{{/if}}\" data-facet-item>\n      <a href=\"{{url}}\" rel=\"nofollow\" data-faceted-search-facet>\n        {{#for 1 5}}\n          <span class=\"rating-star\">\n            <svg class=\"icon icon-list rating-star-{{#if ../param_value '>=' $index}}full{{else}}empty{{/if}}\">\n              <use xlink:href=\"#icon-star\"></use>\n            </svg>\n          </span>\n        {{/for}}\n\n        {{#for 1 2}}\n          <span class=\"rating-star-and-up\">\n            {{../param_value}} {{lang 'faceted_search.and_up'}}\n          </span>\n        {{/for}}\n\n        {{#if ../show_product_counts}}\n          <span class=\"facet-item-count\" data-facet-item-count>({{count}})</span>\n        {{/if}}\n      </a>\n    </li>\n  {{/each}}\n</ul>\n"}
