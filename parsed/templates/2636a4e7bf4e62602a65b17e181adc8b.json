{"components/pages/page-header":"<header class=\"page-header\">\n  {{#if page_title}}\n    <h1 class=\"page-header-title\">{{page_title}}</h1>\n  {{/if}}\n  {{#if theme_settings.pages-show-breadcrumbs}}\n    {{> components/common/breadcrumbs}}\n  {{/if}}\n</header>\n","components/common/breadcrumbs":"<section class=\"breadcrumbs-container\">\n  {{#each breadcrumbs}}\n    <a href=\"{{url}}\">{{name}}</a> {{#unless @last}}/{{/unless}}\n  {{/each}}\n</section>\n"}
