{"components/footer/footer-layout":"<footer class=\"footer-main\" role=\"banner\">\n  {{#any settings.gift_certificates_enabled settings.account_creation_enabled}}\n    <nav class=\"footer-navigation\">\n      {{> components/footer/footer-navigation}}\n    </nav>\n  {{/any}}\n\n  {{#if social_media.length}}\n    <nav class=\"footer-social\">\n      {{> components/footer/footer-social}}\n    </nav>\n  {{/if}}\n\n  {{#if settings.show_newsletter_box}}\n    <nav class=\"footer-newsletter\">\n      {{> components/footer/footer-newsletter}}\n    </nav>\n  {{/if}}\n\n  <div class=\"footer-branding\">\n    {{> components/footer/footer-branding}}\n  </div>\n\n  <div class=\"footer-credits\">\n    {{> components/footer/footer-credits}}\n  </div>\n</footer>\n\n<div class=\"search-form-overlay\" data-search-form-overlay></div>\n","components/footer/footer-navigation":"<ul class=\"footer-navigation-list\">\n  {{#if settings.gift_certificates_enabled}}\n    <li class=\"footer-navigation-list-item\">\n      <a href=\"{{urls.gift_certificate.purchase}}\">\n        <span class=\"footer-navigation-list-item-label\">\n          {{lang 'footer.gift_certificate'}}\n        </span>\n      </a>\n    </li>\n  {{/if}}\n\n  {{#if settings.show_wishlist}}\n    {{#if customer}}\n      <li class=\"footer-navigation-list-item\">\n        <a href=\"{{urls.account.wishlists.all}}\">\n          <span class=\"footer-navigation-list-item-label\">\n            {{lang 'footer.wishlist'}}\n          </span>\n        </a>\n      </li>\n    {{/if}}\n  {{/if}}\n\n  {{#if settings.account_creation_enabled}}\n    {{#if customer}}\n      <li class=\"footer-navigation-list-item\">\n        <a href=\"{{urls.account.index}}\">\n          <span class=\"footer-navigation-list-item-label\">\n            {{lang 'footer.your_account'}}\n          </span>\n        </a>\n      </li>\n\n      <li class=\"footer-navigation-list-item\">\n        <a href=\"{{urls.auth.logout}}\">\n          <span class=\"footer-navigation-list-item-label\">\n            {{lang 'footer.log_out'}}\n          </span>\n        </a>\n      </li>\n    {{else}}\n      <li class=\"footer-navigation-list-item\">\n        <a href=\"{{urls.auth.login}}\">\n          <span class=\"footer-navigation-list-item-label\">\n            {{lang 'footer.log_in'}}\n          </span>\n        </a>\n      </li>\n\n      <li class=\"footer-navigation-list-item\">\n        <a href=\"{{urls.auth.create_account}}\">\n          <span class=\"footer-navigation-list-item-label\">\n            {{lang 'footer.sign_up'}}\n          </span>\n        </a>\n      </li>\n    {{/if}}\n  {{/if}}\n</ul>\n","components/footer/footer-social":"<ul class=\"footer-social-list\">\n  {{#each social_media}}\n    <li class=\"footer-social-list-item\">\n      {{#if display_name '===' 'google+'}}\n        <a href=\"{{url}}\" class=\"google\" target=\"_blank\">\n          <svg class=\"footer-social-list-item-icon\">\n            <use xlink:href=\"#icon-google\">\n          </svg>\n        </a>\n      {{else}}\n        <a href=\"{{url}}\" class=\"{{display_name}}\" target=\"_blank\">\n          <svg class=\"footer-social-list-item-icon\">\n            <use xlink:href=\"#icon-{{display_name}}\">\n          </svg>\n        </a>\n      {{/if}}\n    </li>\n  {{/each}}\n</ul>\n","components/footer/footer-newsletter":"<form class=\"footer-newsletter-form\" action=\"{{urls.subscribe.action}}\" method=\"post\">\n  <input type=\"hidden\" name=\"action\" value=\"subscribe\">\n  <input type=\"hidden\" name=\"nl_first_name\" value=\"bc\">\n  <input type=\"hidden\" name=\"check\" value=\"1\">\n\n  <label class=\"footer-newsletter-label\" for=\"nl_email\">\n    <span class=\"footer-newsletter-label-text show-for-sr\">{{lang 'newsletter.placeholder'}}</span>\n    <input class=\"footer-newsletter-input\" id=\"nl_email\" name=\"nl_email\" type=\"email\" value=\"{{customer.email}}\" placeholder=\"{{lang 'newsletter.placeholder'}}\">\n    <button class=\"footer-newsletter-submit-button\" type=\"submit\">\n      <span class=\"footer-newsletter-submit-label show-for-sr\">{{lang 'newsletter.subscribe'}}</span>\n      <svg class=\"footer-newsletter-submit-icon\"><use xlink:href=\"#icon-chevron-right\"></svg>\n    </button>\n  </label>\n</form>\n","components/footer/footer-branding":"{{#if settings.store_logo.image.data}}\n  <a class=\"footer-branding-logo\" href=\"/\">\n    <img src=\"{{getImage settings.store_logo.image 'logo'}}\" alt=\"{{settings.store_logo.image.alt}}\">\n  </a>\n{{else}}\n  <h1 class=\"footer-branding-text\">\n    <a href=\"/\">{{settings.store_logo.title}}</a>\n  </h1>\n{{/if}}\n","components/footer/footer-credits":"<ul class=\"footer-credits-list\">\n  {{#if theme_settings.footer_copyright}}\n    <li class=\"footer-credits-list-item\">\n      <p>&copy; <script>document.write(new Date().getFullYear())</script> {{settings.store_name}}</p>\n    </li>\n  {{/if}}\n\n  {{#if theme_settings.footer_theme_credits}}\n    <li class=\"footer-credits-list-item\">\n      <p>Scales theme by <a href=\"https://pixelunion.net\" target=\"_blank\" rel=\"nofollow\">Pixel Union</a></p>\n    </li>\n\n    <li class=\"footer-credits-list-item\">\n      <p>Powered by <a href=\"http://www.bigcommerce.com/\" target=\"_blank\" rel=\"nofollow\">BigCommerce</a></p>\n    </li>\n  {{/if}}\n\n  {{#if theme_settings.footer_sitemap}}\n    <li class=\"footer-credits-list-item\">\n      <p><a href=\"{{urls.sitemap}}\">{{lang 'sitemap.title'}}</a></p>\n    </li>\n  {{/if}}\n</ul>\n"}
