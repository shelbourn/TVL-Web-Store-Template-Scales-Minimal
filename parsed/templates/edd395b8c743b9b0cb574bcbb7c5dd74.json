{"components/reviews/review-item":"<article class=\"product-single-review-item\" itemprop=\"review\" itemscope itemtype=\"http://schema.org/Review\">\n  <header class=\"product-single-review-item-header\">\n    <span class=\"product-single-review-item-meta\">\n      <meta itemprop=\"author\" content=\"{{name}}\">\n      {{lang 'product.reviews.written_by_on' name=name date=date}}\n    </span>\n\n    <div class=\"product-single-review-item-stars\" itemprop=\"reviewRating\" itemscope itemtype=\"http://schema.org/Rating\">\n      {{> components/reviews/ratings rating=rating size=\"medium\"}}\n      <span class=\"product-single-review-item-score show-for-sr\" itemprop=\"ratingValue\">{{rating}}/5</span>\n    </div>\n\n    <h3 class=\"product-single-review-item-title\">{{title}}</h3>\n  </header>\n\n  <section class=\"product-single-review-item-body\">\n    <p itemprop=\"reviewBody\">{{text}}</p>\n  </section>\n</article>\n","components/reviews/ratings":"<div class=\"rating-stars\" aria-hidden=\"true\">\n  {{#for 1 5}}\n    {{#if ../this.rating '>=' $index}}\n      <svg class=\"star star-full\">\n        <use xlink:href=\"#icon-star\">\n      </svg>\n    {{else}}\n      <svg class=\"star star-empty\">\n        <use xlink:href=\"#icon-star\">\n      </svg>\n    {{/if}}\n  {{/for}}\n</div>\n"}
