{"components/cart/cart-item/cart-item-quantity":"<div class=\"form-field form-field-number form-field-quantity\" data-cart-item-quantity>\n  <label class=\"form-label\" for=\"quantity\">\n    <span class=\"form-label-text show-for-sr\">{{lang 'cart.quantity'}}</span>\n\n    <div class=\"form-field-control\">\n      <input\n        class=\"quantity-input form-input\"\n        type=\"text\"\n        name=\"qty[]\"\n        value=\"{{quantity}}\"\n        min=\"{{#if product.min_purchase_quantity}}{{product.min_purchase_quantity}}{{else}}0{{/if}}\"\n        max=\"{{#if product.max_purchase_quantity}}{{product.max_purchase_quantity}}{{/if}}\"\n        data-cart-item-quantity-input\n      >\n\n      <div class=\"quantity-toggle-container\">\n        <span class=\"quantity-toggle quantity-increment\" data-cart-item-quantity-change data-cart-item-quantity-increment>\n          <svg class=\"quantity-icon\"><use xlink:href=\"#icon-plus\"></svg>\n        </span>\n\n        <span class=\"quantity-toggle quantity-decrement\" data-cart-item-quantity-change data-cart-item-quantity-decrement>\n          <svg class=\"quantity-icon\"><use xlink:href=\"#icon-minus\"></svg>\n        </span>\n      </div>\n    </div>\n  </label>\n</div>\n"}
