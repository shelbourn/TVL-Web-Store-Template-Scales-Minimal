{"components/common/currency-selector":"<div class=\"currency-selector\" data-currency-selector>\n  <div class=\"form-select-wrapper\">\n    <span class=\"form-selected-text\">\n      {{lang 'footer.currency'}}\n      <span class=\"form-selected-text-currency\">{{currency_selector.active_currency_code}}</span>\n    </span>\n    <select>\n      {{#each currency_selector.currencies}}\n        <option value=\"{{{switch_url}}}\" {{#if is_active}}selected{{/if}}>\n          {{name}}\n        </option>\n      {{/each}}\n    </select>\n  </div>\n</div>\n"}
