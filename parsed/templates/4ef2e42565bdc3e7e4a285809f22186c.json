{"components/product/product-gallery-slideshow":"<div class=\"product-gallery-slideshow {{#if product.images.length '>' 1}}visible-controls{{/if}}\">\n  <div class=\"product-gallery-slideshow-images\" data-slideshow-position=\"0\" data-product-slideshow>\n    {{#if product.images}}\n      {{#each product.images}}\n        <img class=\"product-gallery-slideshow-image\" data-image-position=\"{{@index}}\" src=\"{{getImage this 'large'}}\" alt=\"{{this.alt}}\" data-product-slideshow-image>\n      {{/each}}\n    {{else}}\n      <img class=\"product-gallery-slideshow-image no-image\" data-image-position=\"{{@index}}\" src=\"{{cdn '/assets/img/no-image.svg'}}\" alt=\"{{lang 'product.no_image'}}\" data-product-slideshow-image>\n    {{/if}}\n  </div>\n\n  <div class=\"\n    product-gallery-slideshow-thumbnail-list\n    {{#if product.images.length \"<\" 2}}product-gallery-slideshow-thumbnail-list-single{{/if}}\n  \">\n    {{#if product.images}}\n      {{#each product.images}}\n        <span\n          class=\"product-gallery-slideshow-thumbnail {{#if @first}}active{{/if}}\"\n          style=\"background-image: url({{getImage this 'small'}});\"\n          data-image-position=\"{{@index}}\"\n          data-high-res=\"{{getImage this 'large'}}\"\n          data-product-thumbnail>\n          <img class=\"show-for-sr\" src=\"{{getImage this 'small'}}\" alt=\"{{this.alt}}\">\n        </span>\n      {{/each}}\n    {{else}}\n      <span\n        class=\"product-gallery-slideshow-thumbnail {{#if @first}}active{{/if}}\"\n        style=\"background-image: url({{cdn '/assets/img/no-image.svg'}});\"\n        data-image-position=\"{{@index}}\"\n        data-high-res=\"{{cdn '/assets/img/no-image.svg'}}\"\n        data-product-thumbnail>\n        <img class=\"show-for-sr\" src=\"{{cdn '/assets/img/no-image.svg'}}\" alt=\"{{this.alt}}\">\n      </span>\n    {{/if}}\n  </div>\n</div>\n"}
