{"components/header/header-navigation":"<button class=\"mobile-navigation-trigger\" data-mobile-navigation-trigger>\n  <span class=\"mobile-navigation-trigger-bar\"></span>\n  <span class=\"mobile-navigation-trigger-bar\"></span>\n</button>\n\n<ul class=\"header-navigation-list primary-tier\" data-primary-tier>\n\n  <li class=\"header-navigation-list-item header-search-form\" data-search-form>\n    {{> components/common/search-form}}\n  </li>\n\n  <span class=\"header-navigation-list-overflow\">\n    {{#each categories}}\n      <li class=\"header-navigation-list-item\n        {{#if children}}has-children{{/if}}\n        {{#if ../products}}products-navigation{{/if}}\"\n        {{#if children}}\n          data-dropdown-toggle\n\n          {{#if ../../products}}data-products-toggle{{/if}}\n        {{/if}}>\n\n        <a href=\"{{url}}\">\n          <span class=\"header-navigation-list-item-label\">\n            {{name}}\n\n            {{#if children}}\n              <span class=\"header-navigation-list-item-arrow\">\n                <svg><use xlink:href=\"#icon-chevron-right\"></svg>\n              </span>\n\n              <span class=\"header-navigation-list-item-toggle\">\n                <span class=\"header-navigation-list-item-toggle-bar\"></span>\n                <span class=\"header-navigation-list-item-toggle-bar\"></span>\n              </span>\n            {{/if}}\n          </span>\n        </a>\n\n        {{#if children}}\n          <ul class=\"header-navigation-list secondary-tier\" data-secondary-tier>\n            {{#if ../../products}}\n              <div class=\"header-navigation-product-categories\">\n            {{/if}}\n\n            {{#each children}}\n              <li class=\"header-navigation-list-item {{#if children}}has-children{{/if}}\"\n                {{#if children}}data-dropdown-toggle{{/if}}\n                {{#if ../../../products}}data-products-toggle{{/if}}\n                >\n\n                <a href=\"{{url}}\">\n                  <span class=\"header-navigation-list-item-label\">\n                    {{name}}\n\n                    {{#if children}}\n                      <span class=\"header-navigation-list-item-arrow\">\n                        <svg><use xlink:href=\"#icon-chevron-right\"></svg>\n                      </span>\n\n                      <span class=\"header-navigation-list-item-toggle\">\n                        <span class=\"header-navigation-list-item-toggle-bar\"></span>\n                        <span class=\"header-navigation-list-item-toggle-bar\"></span>\n                      </span>\n                    {{/if}}\n                  </span>\n                </a>\n\n                {{> components/header/header-navigation-recursive products=../../../products}}\n              </li>\n            {{/each}}\n\n            <li class=\"header-navigation-list-item view-all-link\">\n              <a href=\"{{url}}\"><span class=\"header-navigation-list-item-label\">{{lang 'header.navigation.view_all' category=name}}</span></a>\n            </li>\n\n            {{#if ../../products}}\n              </div>\n\n              <div class=\"header-navigation-product-container\" data-header-navigation-products data-quick-shop-scope>\n                {{> components/header/header-navigation-products}}\n              </div>\n            {{/if}}\n          </ul>\n        {{/if}}\n      </li>\n    {{/each}}\n\n    {{#if theme_settings.pages_in_navigation '==' 'pages_in_navigation_main'}}\n      {{#each pages}}\n        <li class=\"header-navigation-list-item {{#if children}}has-children{{/if}}\" {{#if children}}data-dropdown-toggle{{/if}}>\n          <a href=\"{{url}}\">\n            <span class=\"header-navigation-list-item-label\">\n              {{name}}\n\n              {{#if children}}\n                <span class=\"header-navigation-list-item-arrow\">\n                  <svg><use xlink:href=\"#icon-chevron-right\"></svg>\n                </span>\n\n                <span class=\"header-navigation-list-item-toggle\">\n                  <span class=\"header-navigation-list-item-toggle-bar\"></span>\n                  <span class=\"header-navigation-list-item-toggle-bar\"></span>\n                </span>\n              {{/if}}\n            </span>\n          </a>\n\n          {{#if children}}\n            <ul class=\"header-navigation-list secondary-tier\" data-secondary-tier>\n              {{#each children}}\n                <li class=\"header-navigation-list-item {{#if children}}has-children{{/if}}\" {{#if children}}data-dropdown-toggle{{/if}}>\n                  <a href=\"{{url}}\">\n                    <span class=\"header-navigation-list-item-label\">\n                      {{name}}\n\n                      {{#if children}}\n                        <span class=\"header-navigation-list-item-arrow\">\n                          <svg><use xlink:href=\"#icon-chevron-right\"></svg>\n                        </span>\n\n                        <span class=\"header-navigation-list-item-toggle\">\n                          <span class=\"header-navigation-list-item-toggle-bar\"></span>\n                          <span class=\"header-navigation-list-item-toggle-bar\"></span>\n                        </span>\n                      {{/if}}\n                    </span>\n                  </a>\n\n                  {{#if children}}\n                    <ul class=\"header-navigation-list extended-tier\" data-extended-tier>\n                      {{#each children}}\n                        <li class=\"header-navigation-list-item\">\n                          <a href=\"{{url}}\">\n                            <span class=\"header-navigation-list-item-label\">\n                              {{name}}\n                            </span>\n                          </a>\n                        </li>\n                      {{/each}}\n                    </ul>\n                  {{/if}}\n                </li>\n              {{/each}}\n            </ul>\n          {{/if}}\n        </li>\n      {{/each}}\n    {{/if}}\n\n    {{#if theme_settings.pages_in_navigation '==' 'pages_in_navigation_submenu'}}\n      <li class=\"header-navigation-list-item has-children\" data-dropdown-toggle>\n        <a href=\"#\">\n          <span class=\"header-navigation-list-item-label\">\n            {{lang 'header.navigation.more'}}\n\n            <span class=\"header-navigation-list-item-arrow\">\n              <svg><use xlink:href=\"#icon-chevron-right\"></svg>\n            </span>\n\n            <span class=\"header-navigation-list-item-toggle\">\n              <span class=\"header-navigation-list-item-toggle-bar\"></span>\n              <span class=\"header-navigation-list-item-toggle-bar\"></span>\n            </span>\n          </span>\n        </a>\n\n        <ul class=\"header-navigation-list secondary-tier\" data-secondary-tier>\n          {{#each pages}}\n            <li class=\"header-navigation-list-item {{#if children}}has-children{{/if}}\" {{#if children}}data-dropdown-toggle{{/if}}>\n              <a href=\"{{url}}\">\n                <span class=\"header-navigation-list-item-label\">\n                  {{name}}\n\n                  {{#if children}}\n                    <span class=\"header-navigation-list-item-arrow\">\n                      <svg><use xlink:href=\"#icon-chevron-right\"></svg>\n                    </span>\n\n                    <span class=\"header-navigation-list-item-toggle\">\n                      <span class=\"header-navigation-list-item-toggle-bar\"></span>\n                      <span class=\"header-navigation-list-item-toggle-bar\"></span>\n                    </span>\n                  {{/if}}\n                </span>\n              </a>\n\n              {{#if children}}\n                <ul class=\"header-navigation-list extended-tier\" data-extended-tier>\n                  {{#each children}}\n                    <li class=\"header-navigation-list-item\">\n                      <a href=\"{{url}}\">\n                        <span class=\"header-navigation-list-item-label\">\n                          {{name}}\n                        </span>\n                      </a>\n                    </li>\n                  {{/each}}\n                </ul>\n              {{/if}}\n            </li>\n          {{/each}}\n        </ul>\n      </li>\n    {{/if}}\n  </span>\n\n  <li class=\"header-navigation-list-item header-mobile-actions\">{{> components/header/header-mobile-actions}}</li>\n</ul>\n\n<div class=\"header-navigation-overlay\" data-header-navigation-overlay></div>\n","components/common/search-form":"<form class=\"search-form\" action=\"{{urls.search}}\">\n  <label class=\"search-form-label\">\n    <span class=\"search-form-label-text\">{{lang 'search.title'}}</span>\n    <input type=\"text\" class=\"search-form-input form-input\" name=\"search_query\" placeholder=\"{{lang 'search.enter_search_terms'}}\" data-search-input>\n  </label>\n\n  <button class=\"search-form-submit\" type=\"submit\" data-search-submit>\n    <span class=\"search-form-submit-label\">{{lang 'search.submit'}}</span>\n  </button>\n</form>\n","components/header/header-navigation-products":"<h1 class=\"header-navigation-product-title\">\n  <a href=\"{{category.url}}\" title=\"{{category.name}}\">\n    {{#if category.name}}\n      {{lang 'header.navigation.products_all' name=category.name}}\n    {{/if}}\n  </a>\n</h1>\n\n<div class=\"header-navigation-product-list\">\n  {{#if category.products.length}}\n    {{#each (limit category.products 4)}}\n      {{> components/product/product-item overlay=../../theme_settings.collection_products_overlay_type}}\n    {{/each}}\n  {{else}}\n    <p class=\"header-navigation-product-empty\">{{lang 'header.navigation.no_products'}}</p>\n  {{/if}}\n</div>\n","components/header/header-navigation-recursive":"{{#if children}}\n  <ul class=\"header-navigation-list extended-tier\" data-extended-tier>\n\n    {{#each children}}\n      <li class=\"header-navigation-list-item {{#if children}}has-children{{/if}}\"\n        {{#if children}}data-dropdown-toggle{{/if}}\n        {{#if ../../products}}data-products-toggle{{/if}}>\n        <a href=\"{{url}}\" data-url=\"{{url}}\">\n          <span class=\"header-navigation-list-item-label\">\n            {{name}}\n\n            {{#if children}}\n              <span class=\"header-navigation-list-item-arrow\">\n                <svg><use xlink:href=\"#icon-chevron-right\"></svg>\n              </span>\n\n              <span class=\"header-navigation-list-item-toggle\">\n                <span class=\"header-navigation-list-item-toggle-bar\"></span>\n                <span class=\"header-navigation-list-item-toggle-bar\"></span>\n              </span>\n            {{/if}}\n          </span>\n        </a>\n        {{> components/header/header-navigation-recursive}}\n      </li>\n    {{/each}}\n\n    <li class=\"header-navigation-list-item view-all-link\">\n      <a href=\"{{url}}\"><span class=\"header-navigation-list-item-label\">{{lang 'header.navigation.view_all' category=name}}</span></a>\n    </li>\n\n  </ul>\n{{/if}}\n","components/header/header-mobile-actions":"{{#if settings.account_creation_enabled}}\n  <ul class=\"header-mobile-actions-list\">\n    {{#if customer}}\n      <li class=\"header-mobile-actions-list-item\">\n        <a href=\"{{urls.account.index}}\">\n          {{lang 'footer.your_account'}}\n        </a>\n      </li>\n\n      <li class=\"header-mobile-actions-list-item\">\n        <a href=\"{{urls.auth.logout}}\">\n          {{lang 'footer.log_out'}}\n        </a>\n      </li>\n    {{else}}\n      <li class=\"header-mobile-actions-list-item\">\n        <a href=\"{{urls.auth.create_account}}\">\n          {{lang 'footer.sign_up'}}\n        </a>\n      </li>\n\n      <li class=\"header-mobile-actions-list-item\">\n        <a href=\"{{urls.auth.login}}\">\n          {{lang 'footer.log_in'}}\n        </a>\n      </li>\n    {{/if}}\n  </ul>\n{{/if}}\n","components/product/product-item":"<figure class=\"product-item\"\n  {{#if overlay '===' 'collection_procucts_overlay_image_preview'}}\n    data-product-id=\"{{id}}\"\n    data-image-preview\n  {{/if}}>\n\n  <div class=\"product-item-container\"\n    {{#if image}}\n      style=\"background-image: url({{getImage image 'medium'}});\"\n    {{else}}\n      style=\"background-image: url({{cdn '/assets/img/no-image.svg'}});\"\n    {{/if}}\n    >\n\n    <a class=\"product-item-image-link\" href=\"{{url}}\" title=\"{{name}}\">\n      {{#if image}}\n        <img class=\"product-item-image\" src=\"{{getImage image 'thumbnail'}}\" />\n      {{else}}\n        <img class=\"product-item-image\" src=\"{{cdn '/assets/img/no-image.svg'}}\" />\n      {{/if}}\n    </a>\n\n    {{#if overlay '===' 'collection_procucts_overlay_image_preview'}}\n      <div class=\"product-item-image-preview\" data-image-preview-container>\n        {{> components/collection/collection-image-preview}}\n      </div>\n    {{/if}}\n  </div>\n\n  <div class=\"product-item-overlay {{#if out_of_stock_message}}out-of-stock{{/if}}\">\n    <div class=\"product-item-overlay-inner\">\n      <div class=\"product-item-details\">\n        {{#if out_of_stock_message}}\n          <span class=\"product-out-of-stock\">{{out_of_stock_message}}</span>\n        {{else}}\n          {{#if brand.name}}\n            <span class=\"product-item-brand\">{{brand.name}}</span>\n          {{/if}}\n        {{/if}}\n\n        <h5 class=\"product-item-name\">\n          <a href=\"{{url}}\" title=\"{{name}}\">{{name}}</a>\n        </h5>\n\n        <span class=\"product-item-price\">\n          {{> components/product/price context=\"product-item\"}}\n        </span>\n\n        <a class=\"product-item-link\" href=\"{{url}}\" title=\"{{name}}\">\n          <span class=\"product-item-link-label show-for-sr\">{{name}}</span>\n        </a>\n\n        <button class=\"product-item-quick-shop\" data-quick-shop-trigger=\"{{id}}\">\n          <span class=\"product-item-quick-shop-label\">{{lang 'core.product.quickshop.view'}}</span>\n          <svg><use xlink:href=\"#icon-plus\"></svg>\n        </button>\n      </div>\n    </div>\n  </div>\n\n  {{#if price.saved}}\n    <p class=\"product-item-sale-badge\">{{lang 'core.product.sale'}}</p>\n  {{/if}}\n\n  {{#if show_remove}}\n    <form class=\"form product-item-remove\" action=\"{{remove_url}}\" method=\"post\">\n      <button type=\"submit\">\n        <svg><use xlink:href=\"#icon-close\"></svg>\n      </button>\n    </form>\n  {{/if}}\n</figure>\n","components/collection/collection-image-preview":"<div class=\"image-preview-slice-container\">\n  {{#each product.images}}\n    <div class=\"image-preview-slice-image\n      {{#if @index '===' 0}}visible{{/if}}\"\n      style=\"background-image:url('{{getImage this 'medium'}}');\"\n      data-image-index=\"{{@index}}\"></div>\n  {{/each}}\n</div>\n\n<div class=\"image-preview-slice-trigger-container\"></div>\n","components/product/price":"<div itemprop=\"offers\" itemscope itemtype=\"http://schema.org/Offer\">\n  <meta itemprop=\"priceCurrency\" content=\"{{currency_selector.active_currency_code}}\">\n  {{#if price.without_tax}}\n\n    {{#if schema_org}}\n      <meta itemprop=\"price\" content=\"{{price.without_tax.value}}\">\n    {{/if}}\n\n    <div class=\"product-price-container\">\n      <span data-product-price-wrapper=\"without-tax\">\n        <span class=\"product-price {{#if price.saved}}saved{{/if}}\" data-product-price>\n          {{price.without_tax.formatted}}\n        </span>\n\n        {{#if price.saved}}\n          <span class=\"product-price rrp\" data-product-rrp>\n            {{price.rrp_without_tax.formatted}}\n          </span>\n        {{/if}}\n      </span>\n\n      {{#if price.with_tax}}\n        <span class=\"product-price-tax-label\">{{lang 'product.excluding_tax'}}</span>\n      {{/if}}\n    </div>\n  {{/if}}\n\n  {{#if price.with_tax}}\n\n    {{#if schema_org}}\n      {{#unless price.without_tax}}\n        <meta itemprop=\"price\" content=\"{{price.with_tax.value}}\">\n      {{/unless}}\n    {{/if}}\n\n    <div class=\"product-price-container\">\n      <span data-product-price-wrapper=\"with-tax\">\n        <span class=\"product-price {{#if price.saved}}saved{{/if}}\" data-prodct-price>\n          {{price.with_tax.formatted}}\n        </span>\n\n        {{#if price.saved}}\n          <span class=\"product-price rrp\" data-product-rrp>\n            {{price.rrp_with_tax.formatted}}\n          </span>\n        {{/if}}\n      </span>\n\n      {{#if price.without_tax}}\n        <span class=\"product-price-tax-label\">{{lang 'product.including_tax'}}</span>\n      {{/if}}\n    </div>\n  {{/if}}\n\n  {{#if show_savings}}\n    {{#if price.saved}}\n      <span class=\"product-price-saved\" data-product-price-saved>\n        ({{lang 'product.you_save'}} {{price.saved.formatted}})\n      </span>\n    {{/if}}\n  {{/if}}\n</div>\n"}
