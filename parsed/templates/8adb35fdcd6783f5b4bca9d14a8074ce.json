{"components/product/product-gallery-overlay":"<div class=\"product-gallery-overlay\" data-product-gallery-overlay>\n  <section class=\"images-overlay-inner-container\" data-overlay-image-list>\n    {{#each product.images}}\n      <figure class=\"product-gallery-overlay-image\" data-overlay-image data-image-id=\"{{@index}}\">\n        <img src=\"{{getImage this 'zoom'}}\" alt=\"{{this.alt}}\" />\n      </figure>\n    {{/each}}\n  </section>\n\n  <div class=\"product-gallery-overlay-thumbnails\" data-overlay-thumbnail-list>\n    {{#each product.images}}\n      <div class=\"product-gallery-overlay-thumbnails-item\" data-overlay-thumbnail data-find-image-id=\"{{@index}}\">\n        <img src=\"{{getImage this 'small'}}\" alt=\"{{this.alt}}\" />\n      </div>\n    {{/each}}\n  </div>\n\n  <button class=\"images-overlay-close\" data-product-gallery-overlay-close>\n    <svg class=\"icon icon-close\"><use xlink:href=\"#icon-close\" /></svg>\n  </button>\n</div>\n"}
