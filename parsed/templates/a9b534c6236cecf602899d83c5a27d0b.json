{"components/reviews/review-modal":"<div id=\"modal-review-form\" class=\"review-modal-container\">\n  <a href=\"#\" class=\"modal-close\">\n    <svg><use xlink:href=\"#icon-close\"></svg>\n  </a>\n\n  <div class=\"modal-body\">\n    <div class=\"review-image-column\">\n      <figure class=\"review-image-container\">\n        <img src=\"{{getImage product.main_image 'medium' (cdn '/assets/img/no-image.svg')}}\">\n      </figure>\n    </div>\n\n    <div class=\"review-form-column\">\n      <h2 class=\"review-form-heading\">{{lang 'product.reviews.write_review'}}</h2>\n      <form class=\"form\" id=\"form-leave-a-review\" action=\"{{urls.product.post_review}}\" method=\"post\">\n        <input type=\"hidden\" name=\"product_id\" value=\"{{product.id}}\">\n        <input type=\"hidden\" name=\"action\" value=\"post_review\">\n        {{#if product.reviews.throttleToken}}\n          <input type=\"hidden\" name=\"throttleToken\" value=\"{{product.reviews.throttleToken}}\">\n        {{/if}}\n\n        {{#if product.reviews.throttleToken}}\n          <input type=\"hidden\" name=\"throttleToken\" value=\"{{product.reviews.throttleToken}}\" />\n        {{/if}}\n\n        {{!-- Clickable Stars --}}\n        <div class=\"form-field\">\n          <div class=\"form-label-text\">\n            {{lang 'product.reviews.rating_label'}}\n          </div>\n          <fieldset class=\"input-stars form-fieldset\">\n            <input type=\"radio\" id=\"stars-5\" class=\"show-for-sr\" name=\"revrating\" value=\"5\">\n            <label for=\"stars-5\" class=\"star\" title=\"{{lang 'product.reviews.stars_count' NUM=5}}\">\n              <svg class=\"star star-full\">\n                <use xlink:href=\"#icon-star\">\n              </svg>\n              <span class=\"span show-for-sr\">{{lang 'product.reviews.stars_count' NUM=5}}</span>\n            </label>\n            <input type=\"radio\" id=\"stars-4\" class=\"show-for-sr\" name=\"revrating\" value=\"4\">\n            <label for=\"stars-4\" class=\"star\" title=\"{{lang 'product.reviews.stars_count' NUM=4}}\">\n              <svg class=\"star star-full\">\n                <use xlink:href=\"#icon-star\">\n              </svg>\n              <span class=\"span show-for-sr\">{{lang 'product.reviews.stars_count' NUM=4}}</span>\n            </label>\n            <input type=\"radio\" id=\"stars-3\" class=\"show-for-sr\" name=\"revrating\" value=\"3\">\n            <label for=\"stars-3\" class=\"star\" title=\"{{lang 'product.reviews.stars_count' NUM=3}}\">\n              <svg class=\"star star-full\">\n                <use xlink:href=\"#icon-star\">\n              </svg>\n              <span class=\"span show-for-sr\">{{lang 'product.reviews.stars_count' NUM=3}}</span>\n            </label>\n            <input type=\"radio\" id=\"stars-2\" class=\"show-for-sr\" name=\"revrating\" value=\"2\">\n            <label for=\"stars-2\" class=\"star\" title=\"{{lang 'product.reviews.stars_count' NUM=2}}\">\n              <svg class=\"star star-full\">\n                <use xlink:href=\"#icon-star\">\n              </svg>\n              <span class=\"span show-for-sr\">{{lang 'product.reviews.stars_count' NUM=2}}</span>\n            </label>\n            <input type=\"radio\" id=\"stars-1\" class=\"show-for-sr\" name=\"revrating\" value=\"1\">\n            <label for=\"stars-1\" class=\"star\" title=\"{{lang 'product.reviews.stars_count' NUM=1}}\">\n              <svg class=\"star star-full\">\n                <use xlink:href=\"#icon-star\">\n              </svg>\n              <span class=\"span show-for-sr\">{{lang 'product.reviews.stars_count' NUM=1}}</span>\n            </label>\n          </fieldset>\n        </div>\n\n        {{!-- Name --}}\n        {{> core/forms/common/text label=(lang 'product.reviews.form.name') name=\"revfromname\" id=\"revfromname\" required=\"true\" prefill=(lang 'product.reviews.form.name')}}\n\n        {{!-- Email --}}\n        {{> core/forms/common/email label=(lang 'product.reviews.form.email') name=\"email\" id=\"rating-email\" required=\"true\" prefill=(lang 'product.reviews.form.email')}}\n\n        {{!-- Subject --}}\n        {{> core/forms/common/text label=(lang 'product.reviews.form.subject') name=\"revtitle\" id=\"rating-title\" required=\"true\" prefill=(lang 'product.reviews.form.subject')}}\n\n        {{!-- Comments --}}\n        {{> core/forms/common/multiline label=(lang 'product.reviews.form.comments') name=\"revtext\" id=\"rating-comment\" required=\"true\" rows=6 prefill=(lang 'product.reviews.form.comments')}}\n\n        {{#if product.reviews.captcha}}\n          <div class=\"form-field review-form-captcha\">\n            <script type=\"text/javascript\" src=\"https://www.google.com/recaptcha/api/challenge?k={{product.reviews.captcha}}\"></script>\n            <noscript>\n              <iframe src=\"https://www.google.com/recaptcha/api/noscript?k={{product.reviews.captcha}}\" height=\"300\" width=\"500\" frameborder=\"0\"></iframe>\n              <textarea name=\"recaptcha_challenge_field\" rows=\"3\" cols=\"40\"></textarea>\n              <input type=\"hidden\" name=\"recaptcha_response_field\" value=\"manual_challenge\">\n            </noscript>\n          </div>\n        {{/if}}\n\n        <input type=\"submit\" class=\"form-submit button button-primary\" value=\"{{lang 'product.reviews.form.submit'}}\">\n      </form>\n    </div>\n\n  </div>\n</div>\n","core/forms/common/text":"\n<div class=\"form-field form-field-common form-field-text {{#if required}} form-required{{/if}}\" id=\"{{id}}\">\n  <label class=\"form-label\">\n    <span class=\"form-field-title\">\n      {{label}}\n      {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n    </span>\n    <span class=\"form-field-control\">\n      <input class=\"form-input\"\n        id=\"input-{{id}}\"\n        name=\"{{name}}\"\n        type=\"text\"\n        placeholder=\"{{prefill}}\"\n        {{#if value}}value=\"{{value}}\"{{/if}}\n        {{#if private_id}}data-field-type=\"{{private_id}}\"{{/if}}\n        {{#if required}}required{{/if}}\n        aria-required=\"{{required}}\">\n    </span>\n  </label>\n</div>\n","core/forms/common/email":"\n<div class=\"form-field form-field-common form-field-email {{#if required}} form-required{{/if}}\" id=\"{{id}}\">\n  <label class=\"form-label\">\n    <span class=\"form-field-title\">\n      {{label}}\n      {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n    </span>\n    <span class=\"form-field-control\">\n      <input class=\"form-input\"\n        id=\"input-{{id}}\"\n        name=\"{{name}}\"\n        type=\"email\"\n        placeholder=\"{{prefill}}\"\n        {{#if value}}value=\"{{value}}\"{{/if}}\n        {{#if private_id}}data-field-type=\"{{private_id}}\"{{/if}}\n        {{#if required}}required{{/if}}\n        aria-required=\"{{required}}\">\n    </span>\n  </label>\n</div>\n","core/forms/common/multiline":"{{!\n  Textarea form component\n\n  @param validation_message string\n         Custom validation message\n}}\n<div class=\"form-field form-field-common form-field-multiline {{#if required}}form-required{{/if}}\" id=\"{{id}}\">\n  <label class=\"form-label\">\n    <span class=\"form-field-title\">\n      {{label}}\n      {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n    </span>\n    <span class=\"form-field-control\">\n      <textarea\n        class=\"form-input\"\n        id=\"textarea-{{id}}\"\n        name=\"{{name}}\"\n        placeholder=\"{{prefill}}\"\n        rows=\"{{rows}}\"\n        {{#if required}}\n          required\n          {{#if validation_message}}\n            data-vd-message-required=\"{{validation_message}}\"\n          {{/if}}\n        {{/if}}\n        aria-required=\"{{required}}\">{{value}}</textarea>\n    </span>\n  </label>\n</div>\n"}
