{"components/cart/cart-totals":"<h3 class=\"cart-total-title\">{{lang 'cart.summary'}}</h3>\n\n<aside class=\"cart-total-container\">\n\n  <div class=\"cart-total-item cart-subtotal\">\n    <span class=\"cart-total-item-label\">{{lang 'cart.subtotal'}}</span>\n    <span class=\"cart-total-item-value\">{{cart.sub_total.formatted}}</span>\n  </div>\n\n  {{#if cart.discount}}\n    <div class=\"cart-total-item cart-discount\">\n      <span class=\"cart-total-item-label\">{{lang 'cart.discount.title'}}</span>\n      <span class=\"cart-total-item-value\">{{cart.discount.formatted}}</span>\n    </div>\n  {{/if}}\n\n  {{#if cart.gift_wrapping_cost.value '>' 0}}\n    <div class=\"cart-total-item\">\n      <span class=\"cart-total-item-label\">{{lang 'cart.gift_wrapping.title'}}</span>\n      <span class=\"cart-total-item-value\">{{cart.gift_wrapping_cost.formatted}}</span>\n    </div>\n  {{/if}}\n\n  {{> components/cart/discounts/discount-coupon-codes}}\n\n  {{#if settings.gift_certificates_enabled}}\n    {{> components/cart/discounts/discount-gift-certificates}}\n  {{/if}}\n\n  {{#if cart.shipping_handling.show_estimator}}\n    {{> components/cart/shipping/shipping-calculator cart.shipping_handling}}\n  {{/if}}\n\n  {{#each cart.taxes}}\n    <div class=\"cart-total-item cart-tax\">\n      <span class=\"cart-total-item-label\">{{name}}</span>\n      <span class=\"cart-total-item-value\">{{cost.formatted}}</span>\n    </div>\n  {{/each}}\n\n  <div class=\"cart-total-item cart-total\">\n    <span class=\"cart-total-item-label cart-total-label\">{{lang 'cart.total_amount'}}</span>\n    <span class=\"cart-total-item-value cart-total-value \">{{cart.grand_total.formatted}} {{currency_selector.active_currency_code}}</span>\n  </div>\n\n  <div class=\"cart-actions\">\n    <span class=\"cart-actions-item cart-actions-update\">\n      <button class=\"button button-small button-alt\" type=\"button\" data-cart-update>{{lang 'cart.update_cart'}}</button>\n    </span>\n\n    <span class=\"cart-actions-item cart-actions-checkout\">\n      <a class=\"button button-small\" href=\"{{urls.checkout.single_address}}\">{{lang 'cart.check_out'}}</a>\n    </span>\n\n    <div class=\"cart-additional-checkout-buttons\">\n      {{#each cart.additional_checkout_buttons}}\n        {{{this}}}\n      {{/each}}\n    </div>\n  </div>\n\n  <div class=\"cart-messages\" data-cart-messages>\n    {{#if cart.quantity '===' 0}}\n      {{> core/alerts/alert-info (lang 'cart.empty')}}\n    {{/if}}\n\n    {{#if cart.status_messages}}\n      {{#each cart.status_messages}}\n        {{> core/alerts/alert-info message=message dismissable=false}}\n      {{/each}}\n    {{/if}}\n  </div>\n\n</aside>\n","components/cart/discounts/discount-coupon-codes":"{{inject 'couponCodeEmptyInput' (lang 'cart.coupon_codes.empty_error')}}\n\n<div class=\"cart-total-item cart-coupon\" data-coupon-codes>\n  {{#if cart.coupons}}\n    {{#each cart.coupons}}\n      <span class=\"cart-total-item-label\">\n        {{lang 'cart.coupon_codes.label' code=code}}\n      </span>\n\n      <span class=\"cart-total-item-value\">\n        {{discount.formatted}}\n        <a class=\"cart-total-item-edit\" title=\"{{lang 'cart.coupon_codes.remove'}}\" href=\"{{remove_url}}\">\n          {{lang 'cart.coupon_codes.remove'}}\n        </a>\n      </span>\n    {{/each}}\n  {{else}}\n    <span class=\"cart-total-item-label\">\n      {{lang 'cart.coupon_codes.coupon_code'}}\n    </span>\n\n    <button\n      class=\"cart-total-item-toggle\"\n      data-add-text=\"{{lang 'cart.coupon_codes.add_code'}}\"\n      data-cancel-text=\"{{lang 'cart.cancel'}}\"\n      data-coupon-code-toggle>\n      {{lang 'cart.coupon_codes.add_code'}}\n    </button>\n  {{/if}}\n\n  <form class=\"cart-discount-form form\" method=\"post\" action=\"{{urls.cart}}\" data-validated-form data-coupon-code-form>\n    <div class=\"form-field form-field-text\">\n      <label class=\"form-label\" for=\"couponcode\">\n        <span class=\"form-label-text show-for-sr\">{{lang 'cart.coupon_codes.enter_code'}}</span>\n        <input class=\"form-input\" id=\"couponcode\" type=\"text\" name=\"couponcode\" value=\"\" placeholder=\"{{lang 'cart.coupon_codes.enter_code'}}\" required data-coupon-code-input>\n      </label>\n    </div>\n\n    <div class=\"form-actions\">\n      <input class=\"button button-small\" type=\"submit\" value=\"{{lang 'cart.coupon_codes.submit'}}\">\n      <input type=\"hidden\" name=\"action\" value=\"applycoupon\">\n    </div>\n  </form>\n</div>\n","components/cart/discounts/discount-gift-certificates":"{{inject 'giftCertificateInputEmpty' (lang 'cart.gift_certificates.error')}}\n\n<div class=\"cart-total-item cart-gift-certificates\" data-gift-certificates>\n  {{#if cart.gift_certificates}}\n    {{#each cart.gift_certificates}}\n      <div class=\"cart-gift-certificate\">\n        <span class=\"cart-total-item-label\">\n          {{lang 'cart.gift_certificates.label' code=code}}\n          <br>\n          {{lang 'cart.gift_certificates.remaining' balance=remaining.formatted}}\n        </span>\n        <span class=\"cart-total-item-value\">\n          {{used.formatted}}\n          <a class=\"cart-total-item-edit\" href=\"{{remove_url}}\" title=\"{{lang 'cart.gift_certificates.remove'}}\">\n            {{lang 'cart.gift_certificates.remove'}}\n          </a>\n        </span>\n      </div>\n    {{/each}}\n  {{else}}\n    <span class=\"cart-total-item-label\">{{lang 'cart.gift_certificates.gift_certificate'}}</span>\n    <button\n      class=\"cart-total-item-toggle\"\n      data-add-text=\"{{lang 'cart.gift_certificates.add_code'}}\"\n      data-cancel-text=\"{{lang 'cart.cancel'}}\"\n      data-gift-certificate-toggle>\n      {{lang 'cart.gift_certificates.add_code'}}\n    </button>\n  {{/if}}\n\n  <form class=\"cart-discount-form form\" method=\"post\" action=\"{{urls.cart}}\" data-validated-form data-gift-certificate-form>\n    <div class=\"form-field form-field-text\">\n      <label class=\"form-label\" for=\"couponcode\">\n        <span class=\"form-field-title show-for-sr\">{{lang 'cart.gift_certificates.enter_code'}}</span>\n        <input class=\"form-input\" id=\"couponcode\" type=\"text\" name=\"certcode\" value=\"\" placeholder=\"{{lang 'cart.gift_certificates.enter_code'}}\" required data-gift-certificate-input>\n      </label>\n    </div>\n    <div class=\"form-actions\">\n      <input class=\"button button-small\" type=\"submit\" value=\"{{lang 'cart.gift_certificates.submit'}}\">\n      <input type=\"hidden\" name=\"action\" value=\"applycoupon\">\n    </div>\n  </form>\n</div>\n","components/cart/shipping/shipping-calculator":"<div class=\"cart-total-item cart-shipping\" data-cart-shipping>\n  <span class=\"cart-total-item-label\">\n    {{lang 'cart.shipping_calculator.title'}}\n  </span>\n  {{#if shipping_cost}}\n    <span class=\"cart-total-item-value\">\n      {{shipping_cost.formatted}}\n      <button class=\"cart-total-item-edit\" title=\"{{lang 'cart.shipping_calculator.edit_shipping'}}\" data-shipping-calculator-toggle>\n        {{lang 'cart.shipping_calculator.edit_shipping'}}\n      </button>\n    </span>\n  {{else}}\n    <button\n      class=\"cart-total-item-toggle\"\n      data-add-text=\"{{lang 'cart.shipping_calculator.estimate_shipping'}}\"\n      data-cancel-text=\"{{lang 'cart.cancel'}}\"\n      data-shipping-calculator-toggle>\n      {{lang 'cart.shipping_calculator.estimate_shipping'}}\n    </button>\n  {{/if}}\n\n  <form class=\"cart-shipping-form form\" data-validated-form data-shipping-calculator>\n    <div class=\"form-field-control\">\n      <div class=\"form-field form-field-text form-field-country\">\n        <label class=\"form-label\" for=\"shipping-country\">\n          <span class=\"form-field-title show-for-sr\">{{lang 'cart.shipping_calculator.select_a_country'}}</span>\n          <div class=\"form-select-wrapper\">\n            <select class=\"form-select\" id=\"shipping-country\" name=\"shipping-country\" data-field-type=\"Country\" required>\n              <option>{{lang 'cart.shipping_calculator.select_a_country'}}</option>\n              {{#each countries}}\n                <option value=\"{{id}}\" {{#if selected}}selected=\"selected\"{{/if}}>\n                  {{name}}\n                </option>\n              {{/each}}\n            </select>\n          </div>\n        </label>\n      </div>\n\n      <div class=\"form-field form-field-text form-field-state\">\n        <label class=\"form-label\" for=\"shipping-state\">\n          <span class=\"form-field-title show-for-sr\">{{lang 'cart.shipping_calculator.select_a_state'}}</span>\n          <div class=\"form-select-wrapper\">\n            <select class=\"form-select\" id=\"shipping-state\" name=\"shipping-state\" data-field-type=\"State\" required>\n              <option value=\"\">{{lang 'cart.shipping_calculator.select_a_state'}}</option>\n              {{#each states}}\n                <option value=\"{{id}}\" {{#if selected}}selected=\"selected\"{{/if}}>\n                  {{name}}\n                </option>\n              {{/each}}\n            </select>\n          </div>\n        </label>\n      </div>\n\n      <div class=\"form-field form-field-text\">\n        <label class=\"form-label\" for=\"shipping-zip\">\n          <span class=\"form-field-title show-for-sr\">{{lang 'cart.shipping_calculator.zip_postal_code'}}</span>\n          <input class=\"form-input\" type=\"text\" id=\"shipping-zip\" name=\"shipping-zip\" value=\"{{selected_zip}}\" placeholder=\"{{lang 'cart.shipping_calculator.zip_postal_code'}}\" required>\n        </label>\n      </div>\n    </div>\n\n    <div class=\"form-actions\">\n      <button class=\"button button-alt button-small\">{{lang 'cart.shipping_calculator.estimate_shipping'}}</button>\n    </div>\n  </form>\n\n  <div class=\"cart-shipping-quotes\" data-shipping-quotes></div>\n</div>\n","core/alerts/alert-info":"<div class=\"alert alert-info\">\n  {{#if dismissable}}<a class=\"alert-dismiss\">&times;</a>{{/if}}\n\n  <div class=\"alert-message\">\n    {{#if message}}\n      {{{message}}}\n    {{else}}\n      {{{this}}}\n    {{/if}}\n  </div>\n</div>\n"}
