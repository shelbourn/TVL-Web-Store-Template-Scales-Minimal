{"components/footer/footer-newsletter":"<form class=\"footer-newsletter-form\" action=\"{{urls.subscribe.action}}\" method=\"post\">\n  <input type=\"hidden\" name=\"action\" value=\"subscribe\">\n  <input type=\"hidden\" name=\"nl_first_name\" value=\"bc\">\n  <input type=\"hidden\" name=\"check\" value=\"1\">\n\n  <label class=\"footer-newsletter-label\" for=\"nl_email\">\n    <span class=\"footer-newsletter-label-text show-for-sr\">{{lang 'newsletter.placeholder'}}</span>\n    <input class=\"footer-newsletter-input\" id=\"nl_email\" name=\"nl_email\" type=\"email\" value=\"{{customer.email}}\" placeholder=\"{{lang 'newsletter.placeholder'}}\">\n    <button class=\"footer-newsletter-submit-button\" type=\"submit\">\n      <span class=\"footer-newsletter-submit-label show-for-sr\">{{lang 'newsletter.subscribe'}}</span>\n      <svg class=\"footer-newsletter-submit-icon\"><use xlink:href=\"#icon-chevron-right\"></svg>\n    </button>\n  </label>\n</form>\n"}
