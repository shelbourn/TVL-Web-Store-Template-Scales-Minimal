{"components/collection/search/show-more":"{{#if product_results.facets}}\n  {{> components/faceted-search/show-more-facets product_results}}\n{{/if}}\n","components/faceted-search/show-more-facets":"<ul class=\"nav-list faceted-search-option-columns\" data-facet-filter-wrapper>\n  {{#each facets}}\n    {{#if facet '===' ../all_facets}}\n      {{#each items}}\n        <li {{#if selected}}class=\"facet-selected\"{{/if}} data-facet-item>\n          <a href=\"{{url}}\" rel=\"nofollow\" data-faceted-search-facet>\n            {{title}}\n\n            {{#if ../show_product_counts}}\n              <span class=\"facet-item-count\" data-facet-item-count>({{count}})</span>\n            {{/if}}\n          </a>\n        </li>\n      {{/each}}\n    {{/if}}\n  {{/each}}\n</ul>\n"}
